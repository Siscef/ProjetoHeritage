<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />

    <title>@ViewBag.Title</title>

    <link rel="icon" type="image/ico" href="../../Content/aquarius/favicon.ico" />

    <link href="../../Content/aquarius/css/stylesheets.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="../../Content/aquarius/css/fullcalendar.print.css" media="print" />

    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js"></script>
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins/jquery/jquery.mousewheel.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/cookie/jquery.cookies.2.2.0.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/bootstrap.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/charts/jquery.flot.js"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins/charts/jquery.flot.stack.js"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins/charts/jquery.flot.pie.js"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins/charts/jquery.flot.resize.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/sparklines/jquery.sparkline.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/fullcalendar/fullcalendar.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/select2/select2.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/uniform/uniform.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/maskedinput/jquery.maskedinput-1.3.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/validation/languages/jquery.validationEngine-en.js" charset="utf-8"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins/validation/jquery.validationEngine.js" charset="utf-8"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins/animatedprogressbar/animated_progressbar.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/qtip/jquery.qtip-1.0.0-rc3.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/cleditor/jquery.cleditor.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/dataTables/jquery.dataTables.min.js"></script>

    <script type="text/javascript" src="../../Scripts/js/plugins/fancybox/jquery.fancybox.pack.js"></script>

    <script type="text/javascript" src="../../Scripts/js/cookies.js"></script>
    <script type="text/javascript" src="../../Scripts/js/actions.js"></script>
    <script type="text/javascript" src="../../Scripts/js/charts.js"></script>
    <script type="text/javascript" src="../../Scripts/js/plugins.js"></script>

</head>
<body>
    <div class="container-fluid">
        <div id="logindisplay">
            @Html.Partial("_LogOnPartial")
        </div>
    </div>
    <div class="header">
        <a class="logo" href="http://localhost:10112/Administracao/Home/Principal">
            <img src="../../Content/aquarius/img/logo.png" alt="Aquarius -  responsive admin panel" title="Aquarius -  responsive admin panel" /></a>
        <ul class="header_menu">
            <li class="list_icon"><a href="#">&nbsp;</a></li>
        </ul>
    </div>

    <div class="menu">
        @Html.Partial("MenuIndex")
    </div>

    <div class="content">


        <div class="breadLine">

            <ul class="breadcrumb">
                <li><a href="http://localhost:10112/Administracao/Home">Página Inicial</a> <span class="divider"></span></li>


            </ul>


        </div>

        <div class="workplace">

            @*   <div class="row-fluid">
                <div class="span12">

                    <div class="widgetButtons">
                        <div class="bb"><a href="http://localhost:10112/Administracao/Bem/create"><span class="ibw-edit"></span></a></div>
                        <div class="bb">
                            <a href="#"><span class="ibw-folder"></span></a>
                           
                        </div>                      
                       
                        <div class="bb">
                            <a href="#"><span class="ibw-mail"></span></a>
                           
                        </div>
                        <div class="bb"><a href="http://localhost:10112/Administracao/ConfiguracaoSistema/Create"><span class="ibw-settings"></span></a></div>
                    </div>

                </div>
            </div>*@

            <div class="row-fluid">

                <div class="span4">

                    <div class="wBlock red">
                        <div class="dSpace">
                            <h3>Média depreciação</h3>
                            <span class="mChartBar" sparktype="bar" sparkbarcolor="white">
                                <!--130,190,260,230,290,400,340,360,390-->
                            </span>
                            <span class="number">@ViewBag.MediaDepreciacao %</span>
                        </div>
                        <div class="rSpace">
                            <span>R$ @ViewBag.TotalAmortizacaoMensal <b>Total depreciado desse mês</b></span>
                            <span>R$ @ViewBag.TotalPisMensal <b>Total Pis</b></span>
                            <span>R$ @ViewBag.TotalCofinsMensal <b>Total Cofins</b></span>
                        </div>
                    </div>

                </div>
                <div class="span4">

                    <div class="wBlock green">
                        <div class="dSpace">
                            <h3>Bens Cadastrados</h3>
                            <span class="mChartBar" sparktype="bar" sparkbarcolor="white">
                                <!--5,10,15,20,23,21,25,20,15,10,25,20,10-->
                            </span>
                            <span class="number">@ViewBag.TotalBens</span>
                        </div>
                        <div class="rSpace">
                            <span>@ViewBag.TotalBensAtivos <b>ativos</b></span>
                            <span>@ViewBag.TotalBensObsoletos <b>obsoletos</b></span>
                            <span>@ViewBag.TotalBensInativos <b>inativos</b></span>
                        </div>
                    </div>
                    <div class="clear"></div>

                </div>


            </div>




            @* <div class="dr"><span></span></div>*@

            <div class="row-fluid">

                @* @Html.Partial("Noticias")*@




            </div>

            @* <div class="dr"><span></span></div>




            <div class="dr"><span></span></div>*@

        </div>

    </div>

    @RenderBody()

</body>
</html>
