<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />

    <title>@ViewBag.Title</title>

    <link rel="icon" type="image/ico" href="~/Content/aquarius/favicon.ico" />

    <link href="~/Content/aquarius/css/stylesheets.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="~/Content/aquarius/css/fullcalendar.print.css" media="print" />

    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js"></script>
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins/jquery/jquery.mousewheel.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/cookie/jquery.cookies.2.2.0.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/bootstrap.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/charts/jquery.flot.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins/charts/jquery.flot.stack.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins/charts/jquery.flot.pie.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins/charts/jquery.flot.resize.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/sparklines/jquery.sparkline.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/fullcalendar/fullcalendar.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/select2/select2.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/uniform/uniform.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/maskedinput/jquery.maskedinput-1.3.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/validation/languages/jquery.validationEngine-en.js" charset="utf-8"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins/validation/jquery.validationEngine.js" charset="utf-8"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins/animatedprogressbar/animated_progressbar.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/qtip/jquery.qtip-1.0.0-rc3.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/cleditor/jquery.cleditor.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/dataTables/jquery.dataTables.min.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/plugins/fancybox/jquery.fancybox.pack.js"></script>

    <script type="text/javascript" src="~/Content/aquarius/js/cookies.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/actions.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/charts.js"></script>
    <script type="text/javascript" src="~/Content/aquarius/js/plugins.js"></script>

</head>
<body>
    <div class="container-fluid">
        <div id="logindisplay">
            @Html.Partial("_LogOnPartial")
        </div>
    </div>
    <div class="header">
        <a class="logo" href="index.html">
            <img src="~/Content/aquarius/img/logo.png" alt="Aquarius -  responsive admin panel" title="Aquarius -  responsive admin panel" /></a>
        <ul class="header_menu">
            <li class="list_icon"><a href="#">&nbsp;</a></li>
        </ul>
    </div>

    <div class="menu">
        @Html.Partial("MenuIndex")
    </div>

    <div class="content">


        <div class="breadLine">

            <ul class="breadcrumb">
                <li><a href="http://localhost:7618/Administracao/Home">Página Inicial</a> <span class="divider"></span></li>

            </ul>

            <ul class="buttons">
                <li>
                    <a href="#" class="link_bcPopupList"><span class="icon-user"></span><span class="text">Lista Usuários</span></a>

                    <div id="bcPopupList" class="popup">
                        <div class="head">
                            <div class="arrow"></div>
                            <span class="isw-users"></span>
                            <span class="name">Lista Usuários</span>
                            <div class="clear"></div>
                        </div>
                        <div class="body-fluid users">

                            <div class="item">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/Content/aquarius/img/users/aqvatarius.jpg" width="32" /></a>
                                </div>
                                <div class="info">
                                    <a href="#" class="name">Aquarius</a>
                                    <span>online</span>
                                </div>
                                <div class="clear"></div>
                            </div>

                            <div class="item">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/Content/aquarius/img/users/olga.jpg" width="32" /></a>
                                </div>
                                <div class="info">
                                    <a href="#" class="name">Olga</a>
                                    <span>online</span>
                                </div>
                                <div class="clear"></div>
                            </div>

                            <div class="item">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/Content/aquarius/img/users/alexey.jpg" width="32" /></a>
                                </div>
                                <div class="info">
                                    <a href="#" class="name">Alexey</a>
                                    <span>online</span>
                                </div>
                                <div class="clear"></div>
                            </div>

                            <div class="item">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/Content/aquarius/img/users/dmitry.jpg" width="32" /></a>
                                </div>
                                <div class="info">
                                    <a href="#" class="name">Dmitry</a>
                                    <span>online</span>
                                </div>
                                <div class="clear"></div>
                            </div>

                            <div class="item">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/Content/aquarius/img/users/helen.jpg" width="32" /></a>
                                </div>
                                <div class="info">
                                    <a href="#" class="name">Helen</a>
                                </div>
                                <div class="clear"></div>
                            </div>

                            <div class="item">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/Content/aquarius/img/users/alexander.jpg" width="32" /></a>
                                </div>
                                <div class="info">
                                    <a href="#" class="name">Alexander</a>
                                </div>
                                <div class="clear"></div>
                            </div>

                        </div>
                        <div class="footer">
                            <button class="btn" type="button">Add new</button>
                            <button class="btn btn-danger link_bcPopupList" type="button">Close</button>
                        </div>
                    </div>

                </li>
                <li>
                    <a href="#" class="link_bcPopupSearch"><span class="icon-search"></span><span class="text">Search</span></a>

                    <div id="bcPopupSearch" class="popup">
                        <div class="head">
                            <div class="arrow"></div>
                            <span class="isw-zoom"></span>
                            <span class="name">Pesquisar</span>
                            <div class="clear"></div>
                        </div>
                        <div class="body search">
                            <input type="text" placeholder="Some text for search..." name="search" />
                        </div>
                        <div class="footer">
                            <button class="btn" type="button">Pesquisar</button>
                            <button class="btn btn-danger link_bcPopupSearch" type="button">Fechar</button>
                        </div>
                    </div>
                </li>
            </ul>

        </div>

        <div class="workplace">

            <div class="row-fluid">
                <div class="span12">

                    <div class="widgetButtons">
                        <div class="bb"><a href="http://localhost:7618/Administracao/Bem/create"><span class="ibw-plus"></span></a></div>
                        <div class="bb">
                            <a href="#"><span class="ibw-folder"></span></a>
                            <div class="caption red">31</div>
                        </div>
                        <div class="bb"><a href="#"><span class="ibw-plus"></span></a></div>
                        <div class="bb"><a href="#"><span class="ibw-favorite"></span></a></div>
                        <div class="bb">
                            <a href="#"><span class="ibw-mail"></span></a>
                            <div class="caption green">31</div>
                        </div>
                        <div class="bb"><a href="http://localhost:7618/Administracao/ManutencaoBem/Create"><span class="ibw-settings"></span></a></div>
                    </div>

                </div>
            </div>

            <div class="row-fluid">              
            </div>

        </div>

    </div>

    @RenderBody()

</body>
</html>
